{
  "name": "stuv.app_frontend",
  "version": "0.1.7",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t maxdola/stuv.app_frontend:latest -t maxdola/stuv.app_frontend:0.1.7 .",
    "docker:build:ws": "docker build -t maxdola/stuv.app_frontend:webservices .",
    "docker:build:beta": "docker build -t maxdola/stuv.app_frontend:beta -t maxdola/stuv.app_frontend:0.1.7-beta .",
    "docker:push": "docker push maxdola/stuv.app_frontend --all-tags",
    "docker:run": "yarn docker:build && yarn docker:push",
    "docker:run:ws": "yarn docker:build:ws && yarn docker:push",
    "docker:run:beta": "yarn docker:build:beta && yarn docker:push",
    "docker:ma:build": "docker buildx build --platform linux/amd64,linux/arm64 -t maxdola/stuv.app_frontend:latest -t maxdola/stuv.app_frontend:0.1.7 .",
    "docker:ma:prod": "docker buildx build --platform linux/amd64,linux/arm64 --push -t maxdola/stuv.app_frontend:latest -t maxdola/stuv.app_frontend:0.1.7 .",
    "docker:ma:beta": "docker buildx build --platform linux/amd64,linux/arm64 --push -t maxdola/stuv.app_frontend:beta -t maxdola/stuv.app_frontend:0.1.7-beta ."
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@jonkoops/matomo-tracker-react": "^0.7.0",
    "@nivo/core": "^0.73.0",
    "@nivo/pie": "^0.73.0",
    "@tabler/icons": "^1.100.0",
    "@use-gesture/react": "^10.2.20",
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "moment": "^2.29.3",
    "next": "^12.3.1",
    "next-pwa": "^5.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-snowfall": "^1.2.1",
    "tabler-icons-react": "^1.49.0"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/classnames": "^2.3.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "10.4.5",
    "postcss": "^8.4.14",
    "tailwindcss": "3.1.3",
    "typescript": "4.7.4"
  }
